  "paths": {
    "/api/v1/invoice/invoices/health": {
      "get": {
        "tags": [
          "Invoice Extractor",
          "invoices"
        ],
        "summary": "Health Check",
        "description": "Check the health of the invoice API.",
        "operationId": "health_check_api_v1_invoice_invoices_health_get",
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {

                }
              }
            }
          }
        }
      }
